<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <IsPackable>True</IsPackable>
    <Version>1.0.0</Version>
    <PackageId>Eshopworld.Web</PackageId>
    <Title>Eshopworld.Web</Title>
    <Authors>David Rodrigues, eshopworld</Authors>
    <Copyright>Copyright eShopWorld 2017</Copyright>
    <Description>eShopWorld common web artefacts</Description>
    <PackageLicenseUrl>https://github.com/eShopWorld/web/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/eShopWorld/web</PackageProjectUrl>
    <PackageTags>ASPNET Core, Web API, Application Insights, Telemetry, Log</PackageTags>
    <RepositoryUrl>https://github.com/eShopWorld/web</RepositoryUrl>
    <IncludeSource>True</IncludeSource>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
  </PropertyGroup>



  <ItemGroup>
    <PackageReference Include="Eshopworld.Telemetry" Version="1.0.0" />
    <PackageReference Include="JetBrains.Annotations" Version="11.1.0" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Core" Version="2.0.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration.AzureKeyVault" Version="2.0.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration.EnvironmentVariables" Version="2.0.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Json" Version="2.0.0" />
  </ItemGroup>



</Project>
